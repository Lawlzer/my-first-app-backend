	"scripts": {
		"build": "npm run clean && npm run build:types && npm run build:js",
		"build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps true",
		"build:types": "tsc",
		"clean": "rimraf ./dist",
		"lint": "eslint . --cache --fix --cache-location ./temp/eslint-cache",
		"lint:debug": "set TIMING=1 && eslint .",
		"nuke": "cli-confirm \"Do you really want to blow **** up?\" && npm run nuke:confirmed",
		"nuke:confirmed": "rimraf node_modules && rimraf package-lock.json && rimraf ./temp && npm cache clean --force && npm i",
		"prettier": "prettier . --write",
		"prettier:debug": "prettier . --write",
		"start": "nodemon",
		"start:code": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
		"start:production": "cross-env NODE_ENV=production node dist/index",
		"test": "cross-env NODE_ENV=test jest --clearCache && jest"
	},
