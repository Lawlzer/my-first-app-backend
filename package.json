{
	"name": "backend",
	"version": "1.0.0",
	"description": "",
	"keywords": [],
	"license": "ISC",
	"author": "",
	"main": "index.js",
	"scripts": {
		"build": "npm run clean && npm run build:types && npm run build:js",
		"build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --ignore \"**/*.test.ts\" --source-maps true --config-file ./config/babel.config.js",
		"build:types": "tsc --project tsconfig-build.json && tsc-alias --project tsconfig-build.json",
		"clean": "rimraf ./dist",
		"lint": "npm run lint:eslint && npm run lint:prettier",
		"lint:eslint": "eslint . --cache --fix --cache-location ./temp/eslint-cache --ignore-path ./config/.eslintignore --config ./config/.eslintrc.js",
		"lint:eslint:debug": "cross-env TIMING=1 && eslint .",
		"lint:prettier": "prettier . --write --config ./config/.prettierrc.js",
		"lint:prettier:debug": "prettier . --write --config ./config/.prettierc.js",
		"nuke": "cli-confirm \"Do you really want to blow **** up?\" && npm run nuke:confirmed",
		"nuke:confirmed": "npm install -g npm@latest && npm audit fix && rimraf node_modules && rimraf package-lock.json && rimraf ./temp && npm cache clean --force && npm i",
		"pre-commit": "lint-staged -c ./config/lint-staged.config.js",
		"prepare": "husky install",
		"start": "cross-env NODE_ENV=development ts-node-dev --respawn --watch * --ignore-watch node_modules --transpile-only -r tsconfig-paths/register src/index.ts",
		"start:production": "cross-env NODE_ENV=production node dist/index",
		"test": "cross-env NODE_ENV=test jest --clearCache && jest --config ./config/jest.config.ts"
	},
	"prettier": "./config/.prettierrc.js",
	"dependencies": {
		"@lawlzer/helpers": "^3.0.5",
		"@oauth-everything/passport-discord": "^1.0.2",
		"axios": "^0.27.2",
		"connect-mongo": "^4.6.0",
		"cookie-parser": "^1.4.6",
		"cors": "^2.8.5",
		"dotenv": "^16.0.1",
		"express": "^4.18.1",
		"express-session": "^1.17.3",
		"express-slow-down": "^1.4.0",
		"fs-extra": "^10.1.0",
		"mongoose-encryption": "^2.1.2",
		"morgan": "^1.10.0",
		"ms": "^2.1.3",
		"passport": "^0.6.0",
		"passport-github2": "^0.1.12",
		"passport-google-oauth": "^2.0.0",
		"passport-local": "^1.0.0",
		"tsconfig-paths": "^4.1.0",
		"validatorjs": "^3.22.1"
	},
	"devDependencies": {
		"@babel/cli": "^7.18.10",
		"@babel/core": "^7.18.13",
		"@babel/plugin-transform-runtime": "^7.18.10",
		"@babel/preset-env": "^7.18.10",
		"@babel/preset-typescript": "^7.18.6",
		"@babel/types": "^7.18.13",
		"@types/bcrypt": "^5.0.0",
		"@types/cookie-parser": "^1.4.3",
		"@types/cookie-session": "^2.0.44",
		"@types/cors": "^2.8.12",
		"@types/express-session": "^1.17.5",
		"@types/express-slow-down": "^1.3.2",
		"@types/fs-extra": "^9.0.13",
		"@types/jest": "^28.1.8",
		"@types/moment": "^2.13.0",
		"@types/mongoose": "^5.11.97",
		"@types/morgan": "^1.9.3",
		"@types/ms": "^0.7.31",
		"@types/multer": "^1.4.7",
		"@types/node": "^18.7.13",
		"@types/passport": "^1.0.10",
		"@types/passport-discord": "^0.1.6",
		"@types/passport-github2": "^1.2.5",
		"@types/passport-google-oauth": "^1.0.42",
		"@types/passport-local": "^1.0.34",
		"@types/supertest": "^2.0.12",
		"@types/validatorjs": "^3.15.0",
		"@typescript-eslint/eslint-plugin": "^5.35.1",
		"@typescript-eslint/parser": "^5.35.1",
		"babel-cli": "^6.26.0",
		"babel-plugin-module-resolver": "^4.1.0",
		"bcrypt": "^5.0.1",
		"cli-confirm": "^1.0.1",
		"cross-env": "^7.0.3",
		"eslint": "^8.23.0",
		"eslint-plugin-simple-import-sort": "^7.0.0",
		"glob": "^8.0.3",
		"jest": "^29.0.1",
		"lint-staged": "^13.0.3",
		"mongoose": "^6.5.3",
		"prettier": "^2.7.1",
		"prettier-plugin-packagejson": "^2.2.18",
		"rimraf": "^3.0.2",
		"supertest": "^6.2.4",
		"ts-jest": "^28.0.8",
		"ts-node": "^10.9.1",
		"tsc-alias": "^1.7.0",
		"typescript": "^4.8.2"
	}
}
